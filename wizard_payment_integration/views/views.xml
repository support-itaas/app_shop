<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="assets_frontend" inherit_id="website.assets_frontend" name="Assets Frontend">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" href="/wizard_payment_integration/static/src/css/payment_method.css"/>
                <link rel="stylesheet" href="/wizard_payment_integration/static/src/css/wizard_payment_form.css"/>
                <link rel="stylesheet" href="/wizard_payment_integration/static/src/css/payment_details.css"/>
                <link rel="stylesheet" href="/wizard_payment_integration/static/src/css/verification.css"/>
                <link rel="stylesheet" href="/wizard_payment_integration/static/src/css/success.css"/>
                <link rel="stylesheet" href="/wizard_payment_integration/static/src/css/failed.css"/>
            </xpath>

            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/wizard_payment_integration/static/src/js/wizard_payment_form.js"/>
                <script type="text/javascript" src="/wizard_payment_integration/static/src/js/verification.js"/>
                <script type="text/javascript" src="/wizard_payment_integration/static/src/js/scb_call.js"/>
<!--                <script type="text/javascript" src="/wizard_payment_integration/static/src/js/gateway.js"/>-->
            </xpath>
        </template>
    </data>
</odoo>