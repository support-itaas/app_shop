        <field name="model">account.asset.asset</field>
        <field name="inherit_id" ref="account_asset.view_account_asset_asset_form"/>
        <field name="arch" type="xml">
            <button name="set_to_close" position="after">
                <button name="check_and_close" string="Check and Close" type="object"/>
            </button>
            <button name="compute_depreciation_board" position="after">
                <button name="auto_compute" string="Auto Compute" state="draft" type="object"/>
            </button>

        </field>
    </record>




</odoo>