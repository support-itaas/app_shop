<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_wizard_coupon_name" model="ir.sequence">
            <field name="name">Coupon name</field>
            <field name="code">wizard.coupon</field>
            <field name="prefix">CPN/%(range_year)s/</field>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">4</field>
        </record>

        <record id="sequence_wizard_coupon_running" model="ir.sequence">
            <field name="name">Coupon running</field>
            <field name="code">wizard.coupon.running</field>
            <field name="prefix">PKID-%(range_year)s%(month)s/</field>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>