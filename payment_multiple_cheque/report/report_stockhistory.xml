												<!--</td>-->
												<!--<td class="text-right">-->
												<!--</td>-->
											<!--</tr>-->
										<!--</t>-->
									</t>
									<tr>
										<td></td>
										<td>รวม</td>
										<td class="text-right">
											<span t-esc="'{0:,.2f}'.format(float(sum_qty))"/>
										</td>
										<td></td>
										<td></td>
										<td class="text-right">
											<span t-esc="'{0:,.2f}'.format(float(sum_value))"/>
										</td>
										<td></td>
									</tr>

								</tbody>
							</table>
