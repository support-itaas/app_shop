<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="inherit_account_traditional_bank_invoice_form">
        <field name="name">company.bank.form.inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='social_facebook']" position="after">
                <field name="social_instagram"/>
                <field name="social_line"/>
            </xpath>
        </field>
    </record>
</odoo>