<?xml version="1.0"?>
<odoo>
    <record id="location_form" model="ir.ui.view">
        <field name="name">Asset Location Form</field>
        <field name="model">asset.location.line</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <form string="Location Form">
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="sequence_location"/>
                                <field name="date_asset"/>
                                <field name="location_old"/>
                                <field name="location_new"/>
                            </group>
                        </group>
                    </form>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
