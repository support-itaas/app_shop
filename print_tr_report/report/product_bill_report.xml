                        @font-face
                        {
                        font-family: SarabunPSK;
                        src: local('United'), url("/print_tr_report/static/THSarabun.ttf") format('opentype');
                        }
                        .font_fam{
                        font-family: SarabunPSK;
                        src: local('United'), url("/print_tr_report/static/THSarabun.ttf") format('opentype');
                        }

                        table.table-bordered {
                        border:1px solid black;
                        font-family: SarabunPSK;
                        src: local('United'), url("/estimate_dev/static/THSarabun.ttf") format('opentype');
                        }
                        table.table-bordered &gt; thead &gt; tr &gt; th{
                        border:1px solid black;
                        padding:3px;
                        text-align:center;
                        vertical-align: middle;
                        }
                        table.table-bordered &gt; tbody &gt; tr &gt; td{
                        border:0px solid white;
                        border-left: 1px solid black;
                        padding:3px;
                        }
                        table.table-bordered &gt; tfoot &gt; tr &gt; td{
                        border:1px solid black;
                        padding:3px;
                        }
                    </style>

                    <t t-set="max_body_height" t-value="155"/>
                    <t t-set="new_line_height" t-value="4"/>
                    <t t-set="row_line_height" t-value="5"/>

                    <t t-if="o.is_show_lot">
                        <t t-set="max_line_lenght" t-value="40"/>
                    </t>
                    <t t-if="not o.is_show_lot">
                        <t t-set="max_line_lenght" t-value="50"/>
                    </t>

                    <t t-set="break_line" t-value="o.get_break_line_bill(max_body_height,new_line_height,row_line_height,max_line_lenght)"/>
                    <t t-set="start" t-value="0"/>
                    <t t-set="end" t-value="0"/>

                    <t t-foreach="range(0,len(break_line),1)" t-as="x">
                        <table style="height:180mm; font-size: 22px; line-height:18px;" class="table table-bordered">
                            <thead>
                                <t t-if="o.is_show_lot">
                                    <tr>
                                        <th width="5%" rowspan="2">
                                            <span>ลำดับ<br/>NO</span>
                                        </th>
                                        <th colspan="2">
                                            <span>รายการ</span>
                                        </th>
                                        <th width="10%" rowspan="2">
                                            <span>จำนวน</span>
                                        </th>
                                        <th width="11%" rowspan="2">
                                            <span>หน่วย</span>
                                        </th>
                                        <th width="15%" rowspan="2">
                                            <span>Lot No.</span>
                                        </th>
                                        <th width="10%" rowspan="2">
                                            <span>หมายเหตุ</span>
                                        </th>
                                    </tr>
                                    <tr>
                                        <th width="14%">
                                            <span>รหัส</span>
                                        </th>
                                        <th width="35%">
                                            <span>ชื่อสินค้า</span>
                                        </th>
                                    </tr>
                                </t>
                                <t t-if="not o.is_show_lot">
                                    <tr>
                                        <th width="5%" rowspan="2">
                                            <span>ลำดับ<br/>NO</span>
                                        </th>
                                        <th colspan="2">
                                            <span>รายการ</span>
                                        </th>
                                        <th width="10%" rowspan="2">
                                            <span>จำนวน</span>
                                        </th>
                                        <th width="11%" rowspan="2">
                                            <span>หน่วย</span>
                                        </th>
                                        <th width="15%" rowspan="2">
                                            <span>หมายเหตุ</span>
                                        </th>
                                    </tr>
                                    <tr>
                                        <th width="14%">
                                            <span>รหัส</span>
                                        </th>
                                        <th width="45%">
                                            <span>ชื่อสินค้า</span>
                                        </th>
                                    </tr>
                                </t>
                            </thead>
                            <tbody class="sale_tbody" style="font-size: 22px; line-height:18px;">
                                <t t-foreach="range(start,break_line[x],1)" t-as="y">
                                    <t t-if="(y+1)!=(break_line[x])">
                                        <tr height="40px">
                                            <td style="text-align:center">
                                                <span t-esc="y+1"/>
                                            </td>
                                            <td style="text-align:center">
                                                <t t-if="o.move_lines[y].product_id.code">
                                                    <span t-field="o.move_lines[y].product_id.code"/>
                                                </t>
                                                <t t-if="not o.move_lines[y].product_id.code">
                                                    <span>-</span>
                                                </t>
                                            </td>
                                            <td style="text-align:left">
                                                <span t-field="o.move_lines[y].product_id.name"/>
                                            </td>
                                            <td style="text-align:center">
                                                <span t-esc="'{0:,.2f}'.format(float(o.move_lines[y].quantity_done))"/>
                                            </td>
                                            <td style="text-align:center">
                                                <span t-field="o.move_lines[y].product_uom.name"/>
                                            </td>
                                            <t t-if="o.is_show_lot">
                                                <td style="text-align:center">
                                                    <t t-foreach="o.move_lines[y].move_line_ids" t-as="z">
                                                        <span t-if="z.lot_id" t-field="z.lot_id"/>
                                                        <span t-else="" t-field="z.lot_name"/><br/>
                                                    </t>
                                                </td>
                                            </t>
                                            <td style="text-align:right">

                                            </td>
                                        </tr>
                                    </t>
                                    <t t-if="(y+1)==(break_line[x])">
                                        <tr>
                                            <td style="text-align:center">
                                                <span t-esc="y+1"/>
                                            </td>
                                            <td style="text-align:center">
                                                <t t-if="o.move_lines[y].product_id.code">
                                                    <span t-field="o.move_lines[y].product_id.code"/>
                                                </t>
                                                <t t-if="not o.move_lines[y].product_id.code">
                                                    <span>-</span>
                                                </t>
                                            </td>
                                            <td style="text-align:left">
                                                <span t-field="o.move_lines[y].product_id.name"/>
                                            </td>
                                            <td style="text-align:center">
                                                <span t-esc="'{0:,.2f}'.format(float(o.move_lines[y].quantity_done))"/>
                                            </td>
                                            <td style="text-align:center">
                                                <span t-field="o.move_lines[y].product_uom.name"/>
                                            </td>
                                            <t t-if="o.is_show_lot">
                                                <td style="text-align:center">
                                                    <t t-foreach="o.move_lines[y].move_line_ids" t-as="z">
                                                        <span t-if="z.lot_id" t-field="z.lot_id"/>
                                                        <span t-else="" t-field="z.lot_name"/><br/>
                                                    </t>
                                                </td>
                                            </t>
                                            <td style="text-align:right">

                                            </td>
                                        </tr>
                                        <t t-set="end" t-value="(break_line[x])"/>
                                    </t>
                                </t>
                            </tbody>
                            <!--</tfoot>-->
                            <!--if using tfoot it will help to continue the table -->
                        </table>
                        <t t-set="start" t-value="break_line[x]"/>
                        <p style="page-break-after:always;"/>
                    </t>
                </div>

                <div class="footer">
                    <style>
                        @font-face
                        {
                        font-family: SarabunPSK;
                        src: local('United'), url("/print_tr_report/static/THSarabun.ttf") format('opentype');
                        }
                        .font_fam{
                        font-family: SarabunPSK;
                        src: local('United'), url("/print_tr_report/static/THSarabun.ttf") format('opentype');
                        }
                    </style>
                    <div class="last-page">
                        <table width="100%" class="font_fam" style="border:1px solid black; font-size:22px;">
                            <tr>
                                <td width="70%" style="padding-left:10px">
                                    <br/><br/>
                                    <strong>ผู้อนุมัติเบิก_____________________</strong>
                                    <br/><br/>
                                    <strong>ผู้รับ_____________________</strong>
                                    <br/><br/>
                                    <strong>แผนก_____________________</strong><br/><br/>
                                </td>
                                <td width="30%">
                                    <br/><br/>
                                    <strong>ผู้ตรวจสอบ_____________________</strong>
                                    <br/><br/>
                                    <strong>ผู้จ่าย_____________________</strong><br/>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2" style="padding-left:10px">
                                    <div width="100%">
                                        <font style="font-size:20px; text-align:left;">
                                            <span>FM-STO - 01/01 แก้ไขครั้งที่ 01</span>
                                        </font>
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </t>
        </t>
    </template>

